{
  "name": "signflow-pdf-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc && npx playwright install chromium",
    "postinstall": "npx playwright install chromium",
    "prestart": "node -e \"const fs=require('fs');const path=require('path');const src=path.join(process.cwd(),'..','public');const dest=path.join(process.cwd(),'public');if(fs.existsSync(src)){fs.mkdirSync(dest,{recursive:true});['logo-crest.png','logo-text.png','logo.png','logo.jpg','logo.jpeg'].forEach(f=>{const s=path.join(src,f);if(fs.existsSync(s))fs.copyFileSync(s,path.join(dest,f));});}\"",
    "start": "tsx index.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "dotenv": "^17.2.4",
    "express": "^4.21.0",
    "pdf-lib": "^1.17.1",
    "playwright": "^1.49.0",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.0",
    "typescript": "~5.9.3"
  }
}
